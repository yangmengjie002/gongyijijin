<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="default" extends="struts-default">
		<action name="login_*" class="com.action.UserAction" method="{1}">
			<result name="login">/admin/login/home.jsp</result>
		</action>
	</package>
	
	
	<!--王云明的配置文件  -->
	<package name="wangyunming" extends="struts-default">
		<action name="app_*" class="com.wangyunming.figh.action.ApplyingForAction" method="{1}">
			<result name="applyingfor">/MyCenter.jsp</result>
		</action>
		<action name="wang_*" class="com.wangyunming.figh.action.Organization_applicationAction" method="{1}">
			<result name="organizationfor">/proApply.jsp</result>
		</action>
	</package>
</struts>    
