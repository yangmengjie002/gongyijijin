<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<!-- yangmengjie -->
	<package name="default" extends="struts-default">
		<interceptors>
			<interceptor name="empIntercepter" class="com.yangmengjie.interecptor.UserInterecptor">
				<param name="excludeMethods">login</param>
			</interceptor>
			<interceptor-stack name="emploginStack">
				<interceptor-ref name="defaultStack"></interceptor-ref>
				<interceptor-ref name="empIntercepter"></interceptor-ref>
			</interceptor-stack>
		</interceptors>
		<!-- 创建后台登录拦截器栈 -->
		<default-interceptor-ref name="emploginStack"></default-interceptor-ref>
		<!-- 全局配置error到error.jsp页面 -->
		<global-results>
			<result name="error">/error.jsp</result>
		</global-results>
		<action name="emp_*" class="com.yangmengjie.action.EmpAction" method="{1}">
			<result name="success">/admin/login/home.jsp</result>
			<result name="entryBack_error">/entryBack.jsp</result>
			<result name="addEmp">/admin/employee/changeEmployee.jsp</result>
		</action>
	</package>
	
	<package name="city_province" extends="struts-default">
		<action name="get_*" class="com.yangmengjie.action.SelectCityAction" method="{1}">
		
		</action>
	</package>
	
	<!-- yuxiaofei -->
	<package name="yuxiaofei" extends="struts-default">
		<action name="eror_*" class="com.yuxiaofei.pro_apply.action.ProApplyAction" method="{1}">
			<result name="error">/error.jsp</result>
			<result name="right">/right.jsp</result>
		</action>
	</package>
	
	<!-- jingbinbin前端注册 -->
	
	<package name="default1" extends="struts-default">
	 <action name="register_*" class="com.jingbinbinzhu.RegisterAction" method="{1}">
	   <result name="success">/login.jsp</result>
	  
	   <result name="error">/register.jsp</result>
	  
	   <result name="cuowu">error1.jsp</result>
	 </action>
	 
	 <action name="qian_*" class="jingbinbindeng.qianloginAction" method="{1}">
      <result name="qianlogin">/home.jsp</result>
      <result name="dengcuo">/login.jsp</result>	 
	 </action>
	</package>
	
	
</struts>    
