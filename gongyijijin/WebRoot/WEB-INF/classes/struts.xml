<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="default" extends="struts-default">
		<action name="login_*" class="com.action.UserAction" method="{1}">
			<result name="login">/admin/login/home.jsp</result>
		</action>
	</package>
	<package name="yuxiaofei" extends="default">
		<action name="eror_*" class="com.yuxiaofei.pro_apply.action.ProApplyAction" method="{1}">
			<result name="error">/error.jsp</result>
			<result name="right">/right.jsp</result>
		</action>
	</package>
</struts>    
